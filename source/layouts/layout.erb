<!doctype html>
<html>
  <head>
    <meta content="IE=edge" http-equiv="X-UA-Compatible">
    <meta charset="utf-8">
    <meta content="width=device-width,initial-scale=1.0,minimum-scale=1.0,maximum-scale=1.0,user-scalable=no" name="viewport">
    <meta name="description" content="Matt Rothenberg is a Full Stack Designer, based in New York City">
    <meta property="og:title" content="Matt Rothenberg | Full Stack Designer">
    <meta property="og:url" content="http://www.mattrothenberg.com">
    <meta property="og:image" content="https://pbs.twimg.com/profile_images/486132305069477891/aUnjJ9LM.jpeg">
    <link rel="icon" href="/favicon.png" type="image/png" />

    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title || "Matt Rothenberg" %></title>

    <%= stylesheet_link_tag :site %>
  </head>

  <body class="<%= page_classes %> flex-l flex-column">
    <%= partial 'header' %>
    <main class="flex-auto">
      <%= yield %>
    </main>
    <%= partial 'footer' %>
    <script>
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

      ga('create', 'UA-9484500-2', 'auto');
      ga('send', 'pageview');

    </script>
  </body>
</html>
